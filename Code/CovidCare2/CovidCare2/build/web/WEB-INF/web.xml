<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>Controller.RegisterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserInfoController</servlet-name>
        <servlet-class>Controller.UserInfoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>Controller.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewsController</servlet-name>
        <servlet-class>Controller.NewsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TutorialController</servlet-name>
        <servlet-class>Controller.TutorialController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewsDetailsController</servlet-name>
        <servlet-class>Controller.NewsDetailsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageAccountController</servlet-name>
        <servlet-class>Controller.ManageAccountController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManagementController</servlet-name>
        <servlet-class>Controller.ManagementController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateNewsController</servlet-name>
        <servlet-class>Controller.CreateNewsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAccountController</servlet-name>
        <servlet-class>Controller.DeleteAccountController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListNews</servlet-name>
        <servlet-class>Controller.viewListNewsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateTimetable</servlet-name>
        <servlet-class>Controller.CreateTimetable</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>*.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewsController</servlet-name>
        <url-pattern>/news</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TutorialController</servlet-name>
        <url-pattern>/tutorial</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserInfoController</servlet-name>
        <url-pattern>/userinfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewsDetailsController</servlet-name>
        <url-pattern>/NewsDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageAccountController</servlet-name>
        <url-pattern>/ManageAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManagementController</servlet-name>
        <url-pattern>/management</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateNewsController</servlet-name>
        <url-pattern>/CreateNews</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAccountController</servlet-name>
        <url-pattern>/DeleteAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListNews</servlet-name>
        <url-pattern>/viewListNewsController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateTimetable</servlet-name>
        <url-pattern>/CreateTimetable</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>view/home.jsp</welcome-file>
    </welcome-file-list>
    <error-page>
        <location>/view/error.jsp</location>
    </error-page>
</web-app>
